{
  "compilerOptions": {
    "strictNullChecks": true,
    "outDir": "dist",
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "strict": true,
    "baseUrl": "src",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "paths": {
      "./hello_wasm.js": ["./node_modules/hello_wasm"]
    }
  }
}
